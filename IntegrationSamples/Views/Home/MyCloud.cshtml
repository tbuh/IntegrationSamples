                    <tr>
                        <td>@(i + 1).</td>
                        @if (Model.Count > i)
                        {
                            <td>@Model[i].Score</td>
                            <td>@Model[i].Name</td>
                        }
                        else
                        {
                            <td></td>
                            <td></td>
                        }
                    </tr>
                }
            </table>
        </div>
    </div>
