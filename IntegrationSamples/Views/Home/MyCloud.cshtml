
@{
    Layout = null;
}


<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/agentReply.css" />
    <title>Agent reply</title>

</head>

<body>
    <div class="container">
        <div class="leftpane pane-shadow">
            <h3>Facebook part</h3>
            <div style="background:#5c5a5a;-webkit-text-fill-color:  white;">
                <div style="font-size:23px;margin-bottom:20px;">
                    <label>High Scores</label>
                </div>

                <table>

                    <tr>
                        <th>Place</th>
                        <th>Score</th>
                        <th>Phone number</th>
                    </tr>
                    <tr>
                        <td width="10%">1st</td>
                        <td width="45%"></td>
                        <td width="45%"></td>
                    </tr>
                    <tr>
                        <td>2nd</td>
                        <td>23</td>
                        <td>23</td>
                    </tr>
                    <tr>
                        <td>3rd</td>
                        <td></td>
                        <td></td>
                    </tr>
                    <tr>
                        <td>4th</td>
                        <td></td>
                        <td></td>
                    </tr>
                    <tr>
                        <td>5th</td>
                        <td></td>
                        <td></td>
                    </tr>
                </table>
            </div>
        </div>
        <div class="middlepane pane-shadow">
            <p>General</p>
            <div class="grid-container-general">
                <div class="grid-item-general">
                    <a target="_blank" href="https://www.google.com">
                        
                            <img src="~/Content/Images/Live support.PNG" />
                        
                    </a>
                </div>
                <div class="grid-item-general">
                    <a target="_blank" href="https://www.google.com">
                        
                            <img src="~/Content/Images/Supportb.PNG" />
                        
                    </a>
                </div>
                <div class="grid-item-general">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <img src="~/Content/Images/News.PNG" />
                        </div>
                    </a>
                </div>

                <div class="grid-item-general">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <img src="~/Content/Images/Home delivery.PNG" />
                        </div>
                    </a>
                </div>
                <div class="grid-item-weather">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <img src="~/Content/Images/Weather.PNG" />
                            <img src="~/Content/Images/Cell.PNG" />
                        </div>
                    </a>
                </div>
                <div class="grid-item-iAmhungry">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <img  style="width:120px"src="~/Content/Images/Iamhungry.PNG" />
                        </div>
                    </a>
                </div>
            </div>
        </div>
        <div class="rightpane pane-shadow">

            <p>Company feed</p>
            <div class="grid-container-feed">
                <div class="grid-item">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <i class="fa fa-bell image-item"></i>
                            <h4>Late/Absent</h4>
                        </div>
                    </a>
                </div>
                <div class="grid-item">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <i class="fa fa-home image-item" style="color:cornflowerblue;"></i>
                            <h4>Time Off</h4>
                        </div>
                    </a>
                </div>
                <div class="grid-item">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <i class="fa fa-rss-square" style="font-size:30px;color:orange;"></i>
                            <h4>Company news</h4>
                        </div>
                    </a>
                </div>
                <div class="grid-item">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <i class="fa fa-clock-o" style="font-size:30px;color:green"></i>
                            <h4>Clock</h4>
                        </div>
                    </a>
                </div>
                <div class="grid-item">
                    <a target="_blank" href="https://www.google.com">
                        <div>
                            <i class="fa fa-calendar" style="font-size:30px;color:green"></i>
                            <h4>Time sheet</h4>
                        </div>
                    </a>
                </div>
            </div>
            
            <div class="orders">
                <p>Select service</p>
                <div class="grid-container">
                    <div class="grid-item-orders">
                        <div>
                            <i class="material-icons left" style="font-size:30px">stay_current_portrait</i>
                            <p class="right">PREPAID</p>
                        </div>

                    </div>
                    <div class="grid-item-orders">
                        <div>
                            <i class="material-icons left" style="font-size:30px">stay_current_portrait</i>
                            <p class="right">POSTPAID</p>
                        </div>
                    </div>
                    <div class="grid-item-orders">
                        <div>
                            <i class="material-icons left" style="font-size:30px">laptop_mac</i>
                            <p class="right">DTH</p>
                        </div>
                    </div>
                    <div class="grid-item-orders">
                        <div>
                            <i class="material-icons left" style="font-size:30px;transform: rotate(40deg);">battery_std</i>
                            <p class="right">DATA CARD</p>
                        </div>
                    </div>
                    <div class="grid-item-orders">
                        <div>
                            <i class="material-icons left" style="font-size:30px">call</i>
                            <p class="right">LANDLINE</p>
                        </div>

                    </div>
                    <div class="grid-item-orders">
                        <div>
                            <i class="material-icons left" style="font-size:30px">warning</i>
                            <p class="right">ELECTRICITY</p>
                        </div>
                    </div>
                    <div class="grid-item-orders">
                        <div>
                            <i class="material-icons left" style="font-size:30px">local_gas_station</i>
                            <p class="right">GAS</p>
                        </div>

                    </div>
                    <div class="grid-item-orders">
                        <div>
                            <i class="fa fa-hand-stop-o left" style="font-size:30px;float:left;"></i>
                            <p class="right">INSURANCE</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
