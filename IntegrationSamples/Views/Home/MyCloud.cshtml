<div class="wrapper">
    <div class="popup">
        <iframe src="https://www.youtube.com/embed/A6XUVjK9W4o" allowfullscreen>
            <p>Your browser does not support iframes.</p>
        </iframe>
        <a href="#" class="close">X</a>

    </div>
</div>

@section scripts {
    <script>
        $(document).ready(function () {
            $(".popup").hide();
            $(".openpop").click(function (e) {
                e.preventDefault();
                $("iframe").attr("src", $(this).attr('href'));
                $(".links").fadeOut('slow');
                $(".popup").fadeIn('slow');
            });

            $(".close").click(function () {
                $(this).parent().fadeOut("slow");
                $(".links").fadeIn("slow");
            });
        });
    </script>
}
